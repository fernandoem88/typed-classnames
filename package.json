{
  "name": "typed-classnames",
  "version": "1.0.0",
  "homepage": "https://www.linkedin.com/in/fernando-ekutsu-mondele-67405484/",
  "author": {
    "name": "Fernando Ekutsu Mondele",
    "email": "vs_fernando@yahoo.fr",
    "url": "https://github.com/fernandoem88"
  },
  "description": "react css components loader - easy way to use css module in react components with typescript",
  "license": "MIT",
  "repository": "https://github.com/fernandoem88/typed-classnames",
  "main": "dist/loader.umd.js",
  "source": "src/loader/index.js",
  "exports": {
    ".": "./dist/loader.umd.js",
    "./core": "./dist/rcc-core.umd.js",
    "./dist/rcc-core.umd": "./dist/rcc-core.umd.js"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle ./src/loader/*.js ./src/rcc/*.tsx ./src/typings/index.d.ts --jsx React.createElement",
    "start": "microbundle watch",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "example": "node ./example/index.js"
  },
  "peerDependencies": {
    "path": ">= 0.12.7",
    "react": ">= 16.0.0",
    "sass": ">= 1.54.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.6.4",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle": "^0.15.1",
    "microbundle-crl": "^0.13.10",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "sass": "^1.54.3",
    "typescript": "^3.7.5"
  },
  "keywords": [
    "react",
    "css",
    "components",
    "typescript",
    "css-in-js",
    "typed-css-module"
  ],
  "files": [
    "dist"
  ]
}
